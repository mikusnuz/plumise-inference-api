# Server
PORT=3200
NODE_ENV=development

# JWT (CHANGE THESE IN PRODUCTION!)
JWT_SECRET=
JWT_EXPIRATION=24h

# OpenAI Compatibility
# API key for /v1/chat/completions and /v1/models endpoints
# If not set, these endpoints are accessible without authentication
OPENAI_COMPAT_API_KEY=

# Plumise Chain
CHAIN_RPC_URL=http://localhost:26902
CHAIN_WS_URL=ws://localhost:26912
CHAIN_ID=41956
# Gateway private key for signing useCredits transactions
# This address must be set as the gateway in InferencePayment contract
GATEWAY_PRIVATE_KEY=

# Contract Addresses (to be set after deployment)
AGENT_REGISTRY_ADDRESS=
REWARD_POOL_ADDRESS=0x1000
# InferencePayment contract for Pro tier credit management
# If not set, all users default to free tier with no credit deduction
INFERENCE_PAYMENT_ADDRESS=

# Node Discovery
ORACLE_API_URL=http://localhost:15481
NODE_URLS=http://localhost:31330,http://192.168.0.200:31330

# Rate Limiting
FREE_TIER_LIMIT=10
FREE_TIER_WINDOW=3600
FREE_TIER_MAX_TOKENS=2048
PRO_TIER_MAX_TOKENS=4096

# CORS
CORS_ORIGINS=http://localhost:3000,https://dashboard.plumise.com
